package com.beca.ejercicio6;

public class ClasePrincipal {
	
	public InnerClass nuevaInstanciaInnerClass(){
		return new InnerClass();
	}
	
	public InnerStaticClass nuevaInstanciaInnerStaticClass(){
		return new InnerStaticClass();
	}

	static class InnerStaticClass{
		
		String dato;
		
		public InnerStaticClass(){
			dato = "Dato de " + this.getClass().getSimpleName();
		}
		
	}
	
	class InnerClass{
		
		String dato;
		
		public InnerClass(){
			dato = "Dato de " + this.getClass().getSimpleName();
		}
		
	}
}
