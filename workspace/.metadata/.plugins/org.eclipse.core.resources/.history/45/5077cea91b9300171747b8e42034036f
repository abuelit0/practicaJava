package com.beca.flujos;

import com.beca.util.Utilidades;

public class Main {
	
	public static void main(String[] args) {
		
		
		System.out.println(Utilidades.LeeCadena());
		_switch();
	}
	
	private static void _switch(){
		char sex = 'x';
		
		switch (sex) {
		case 'M':
			System.out.println("I am a male");
			//break;

		case 'F':
			System.out.println("I am a female");
			//break;
		/*default:
			System.out.println("I am what i am");
			break;*/
		}
	}
}
