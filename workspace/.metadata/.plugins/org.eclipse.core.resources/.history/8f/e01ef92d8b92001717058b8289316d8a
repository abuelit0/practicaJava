package com.beca.ejemplo;

public class UnaryOperator {
	
	public static void main(String[] args) {
		int num = 10;
		
		System.out.println("Incrementando/Decrementando");
		System.out.println(++num);
		System.out.println(--num);
		System.out.println(num++);
		System.out.println(num--);
		System.out.println(num);
		
		System.out.println("Seting sing...");
		System.out.println(+num);
		System.out.println(-num);
	}

}
