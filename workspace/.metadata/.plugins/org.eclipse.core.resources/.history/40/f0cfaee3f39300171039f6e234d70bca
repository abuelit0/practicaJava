package com.beca.ejemplos;

public class Constructores extends Ejemplos{

	@Override
	public void ejecutaEjemplo(String key) {
		_ejemplo1();
	}
	
	private void _ejemplo1(){
		MiClase objeto = new MiClase();
		MiClase objeto2 = new MiClase("Este mensaje se manda desde el constructor");
	}

}

class MiClase{
	
	public MiClase() {
		System.out.println("Mensaje desde el constructor");
	}
	
	MiClase(String mensaje){
		System.out.println(mensaje);
	}
}