package com.beca.ejemplos;

import java.util.HashMap;

import com.beca.interfaz.IEjemplos;

public abstract class Ejemplos implements IEjemplos{
	
	private HashMap<String, IEjemplos> _ejemplos = new HashMap<String, IEjemplos>();

	private void iniciaEjemplos(){
		_ejemplos.put("Flujos", (IEjemplos)new Flujos());
		_ejemplos.put("Excepciones", (IEjemplos)new Excepciones());
	}
	
	private void _ejecutaEjemplo(String key){
		_ejemplos.get(key).ejemplo();;
	}
}
