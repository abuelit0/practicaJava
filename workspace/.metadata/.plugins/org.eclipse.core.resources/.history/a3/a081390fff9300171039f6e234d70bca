package com.beca.ejercicio5;

public class AppMainEjercicio5 {
	
	public static void main(String[] args) {
		
	}
	
	private void mueve2Da3D(String[][] A2D, String[][][] A3D){
		
		for(int i = 0; i < A2D.length; i++)
			for(int j = 0; j < A2D[i].length; i++){
				A3D[i][j][0] = A2D[i][j];
			}
	}
	
	private void _imprime3D(String [][][] A3D){
		
		for (String[][] is : A3D) {
			for (String[] is2 : is) {
				for (String i : is2) {
					System.out.print(i + " ");
				}
				System.out.println();
			}
			System.out.println();
		}
	}
	
	private void _imprime2D(String [][] A2D){
		
		for (String[] is : A2D) {
			for (String i : is) {
				System.out.print(i + " ");
			}
			System.out.println();
		}
	}
	
	private void _inicializa2D(String [][] A2D){
		for(int i = 0; i < A2D.length; i++)
			for(int j = 0; j < A2D[i].length; j++)
				A2D[i][j] =  i + "," + j;
	}

}
