package com.beca.ejercicio13;

public class AppMainEjercicio13 {

	public static void main(String[] args) {
		
	}
	
	
	public void administraEntrada(Persona cliente) throws MenorDeEdadException, AdolecenteException{
		
		if(cliente.get_edad() < 12)
			throw new MenorDeEdadException(cliente);
		
		if(cliente.get_edad() < 18)
			throw new AdolecenteException(cliente);
		
		System.out.println("Bienvenido: " + cliente);
		
	}
	
}
