package com.beca.metodos;

import com.beca.clasesAdicionales.MiObjeto;

public class PasoDeParametros {

	public static void ejemplo(){
		
		int valInt = 1;
		
		MiObjeto objeto = new MiObjeto();
		
		System.out.println(objeto);
		
		_metodoReferencia(objeto);
		
		System.out.println(objeto);
		
	}
	
	private static void _metodoReferencia(MiObjeto m){
		m.setValorCadena("Nuevo valor");
		m.setValorEntero(10);
	}
	
	private static void _metodoParametro(){
		
	}
	
}
