package com.beca.ejercicio8;

public class AppMainEjercicio8 {

	public static void main(String[] args) {
		
	}
	
	public static void metodo(){
		
		do{
			
			etiqueta:
			for(;;){
				
				while(true){
					
					if(true){
						continue etiqueta;
					}else{
						break etiqueta;
					}
					
				}
			}
			
		}while(true);
	}
}
