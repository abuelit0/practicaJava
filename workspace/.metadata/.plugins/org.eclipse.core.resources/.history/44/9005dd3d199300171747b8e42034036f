package com.beca.flujos;

public class AppMain {
	
	public static void main(String[] args) {
		
	}
	
	public void _switch(){
		char sex = 'M';
		
		switch (sex) {
		case 'M':
			System.out.println("I am a male");
			break;

		case 'F':
			System.out.println("I am a female");
			break;
		default:
			System.out.println("I am what i am");
			break;
		}
	}
}
