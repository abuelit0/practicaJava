package com.beca.ejercicio13;

public class Persona {
	
	private int _edad;
	private String _nombre;
	private Persona _padre;
	
	public Persona(int edad, String nombre){
		
		this._edad = edad;
		this._nombre = nombre;
	}
	
	public Persona(int edad, String nombre, Persona padre){
		this(edad, nombre);

		this._padre = padre;
	}

}
