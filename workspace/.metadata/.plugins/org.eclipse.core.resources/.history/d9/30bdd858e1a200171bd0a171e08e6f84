package com.beca.clasesAdicionales.herencia;

/**
 * Clase hija que extiende de clase padre
 * @author HP
 *
 */
public class Hija extends Padre {

	/**
	 * Constructor vacio de la calse
	 */
	public Hija() {
		System.out.println("La Hija");
	}
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "Soy un objeto Hija :D";
	}
	
}
