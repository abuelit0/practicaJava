package com.beca.ejemplos;

import com.beca.clasesAdicionales.Auto;
import com.beca.clasesAdicionales.Avion;
import com.beca.clasesAdicionales.Bote;
import com.beca.clasesAdicionales.Vehiculo;

public class Casting extends Ejemplos{
	
	public void ejecutaEjemplo(String key) {
		
	}

	private static void ejemplo(){
		
		Vehiculo v = new Vehiculo();
		Auto c = new Auto();
		Bote b = new Bote();
		Avion p = new Avion();
		Vehiculo vc = new Auto();
		
		v = c;
		c = (Auto) v;
		//b = p;
		
		b = (Bote)v;
		c = (Auto) vc;
		
	}


}
