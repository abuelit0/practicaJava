package com.beca.clasesAdicionales;

public class Persona{
	
	void talk(){
		System.out.println("blah, blah...");
	}
	
	void jump(int times){
		for(int i = 0; i < times; i++){
			System.out.println("whoop!");
		}
	}
	
	String tellAge(){
		return "I'm " + getAge();
	}
	
	int getAge(){
		return 10;
	}
}