package com.beca.ejercicio6;

public class ClasePrincipal {

	static class InnerStaticClass{
		
		String dato;
		
		public InnerStaticClass(){
			dato = "Dato de " + this.getClass().getSimpleName();
		}
		
		public String toString(){
			return dato;
		}
		
	}
	
	class InnerClass{
		
		String dato;
		
		public InnerClass(){
			dato = "Dato de " + this.getClass().getSimpleName();
		}
		
		public String toString(){
			return dato;
		}
		
	}
}
